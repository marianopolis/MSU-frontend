{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": ["es6"],
    "jsx": "react-native",
    "outDir": "./outputs",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
